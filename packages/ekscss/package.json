  },
  "dependencies": {
    "@types/stylis": "^4.2.7",
    "source-map-js": "^1.2.1",
    "stylis": "^4.3.6"
  },
  "devDependencies": {
    "@ekscss/plugin-null": "workspace:*",
    "dts-buddy": "0.6.2",
    "esbuild": "0.25.8"
  }
